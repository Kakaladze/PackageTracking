{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules":{
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle": ["error", { "allow": ["_id", "_message"] }],
        "max-len": ["error", { "code": 172 }]
    }
}